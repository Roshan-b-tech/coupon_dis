# MongoDB connection string (replace with your production MongoDB URI)
MONGODB_URI=mongodb+srv://your-username:your-password@your-cluster.mongodb.net/your-database

# Port for the server (usually provided by the hosting platform)
PORT=5000

# Client URL (replace with your Netlify URL once deployed)
CLIENT_URL=https://your-app.netlify.app

# Node environment
NODE_ENV=production 